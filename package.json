{
  "name": "sql-to-nosql-analyzer",
  "private": true,
  "workspaces": [
    "packages/*",
    "docs-site"
  ],
  "version": "1.0.0",
  "scripts": {
    "analyze": "yarn workspace @s2n/cli analyze",
    "build:core": "yarn workspace @s2n/core build",
    "build:llm": "yarn workspace @s2n/llm build",
    "build:cli": "yarn workspace @s2n/cli build",
    "build": "yarn run build:core && yarn run build:llm && yarn run build:cli",
    "docs:dev": "yarn workspace sql2nosql-docs dev",
    "docs:build": "yarn workspace sql2nosql-docs build",
    "docs:preview": "yarn workspace sql2nosql-docs preview"
  },
  "license": "MIT",
  "author": "amin uddin",
  "dependencies": {
    "mongodb": "^7.1.0"
  }
}
